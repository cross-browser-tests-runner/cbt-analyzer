{
  "test_file": "tests/html/jasmine/tests.html",
  "framework": "jasmine",
  "retries": 3,
  "limit": "4mb",
  "capabilities": {
    "CrossBrowserTesting": {
      "local": true,
      "localIdentifier": "{conf}",
      "screenshots": true,
      "video": true,
      "timeout": 60
    }
  },
  "parallel": {
    "CrossBrowserTesting": 4
  },
  "server": {
    "port": 8000,
    "host": "build.cross-browser-tests-runner.org"
  }
}
