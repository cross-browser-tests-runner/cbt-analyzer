{
  "test_file": "tests/html/jasmine/tests.html",
  "framework": "jasmine",
  "retries": 3,
  "limit": "4mb",
  "capabilities": {
    "CrossBrowserTesting": {
      "local": true,
      "localIdentifier": "samsung-browser",
      "screenshots": true,
      "video": true,
      "timeout": 60
    }
  },
  "parallel": {
    "CrossBrowserTesting": 5
  },
  "server": {
    "port": 8000,
    "host": "build.cross-browser-tests-runner.org"
  },
  "browsers": {
    "CrossBrowserTesting": {
      "JS": [
        {
          "os": "Android",
          "osVersion": "5.0",
          "browser": "Samsung Browser",
          "browserVersion": "6.2",
          "device": "Android Galaxy S6"
        },
        {
          "os": "Android",
          "osVersion": "7.0",
          "browser": "Samsung Browser",
          "browserVersion": "6.2",
          "device": "Android Galaxy S7"
        }
      ]
    }
  }
}