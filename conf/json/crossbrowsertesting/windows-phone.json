{
  "test_file": "tests/html/jasmine/tests.html",
  "framework": "jasmine",
  "retries": 1,
  "limit": "4mb",
  "capabilities": {
    "CrossBrowserTesting": {
      "local": true,
      "screenshots": true,
      "timeout": 180
    }
  },
  "parallel": {
    "CrossBrowserTesting": 5
  },
  "server": {
    "port": 8000,
    "host": "build.cross-browser-tests-runner.org"
  },
  "browsers": {
    "CrossBrowserTesting": {
      "JS": [
        {
          "os": "Windows Phone",
          "osVersion": "8",
          "browser": "IE Mobile",
          "browserVersion": "10.0",
          "device": "Win Phone 8 Simulator"
        },
        {
          "os": "Windows Phone",
          "osVersion": "8.1",
          "browser": "IE Mobile",
          "browserVersion": "11.0",
          "device": "Win Phone 8.1 Simulator"
        }
      ]
    }
  }
}
