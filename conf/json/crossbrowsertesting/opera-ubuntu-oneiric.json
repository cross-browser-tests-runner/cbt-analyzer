{
  "test_file": "tests/html/jasmine/tests.html",
  "framework": "jasmine",
  "retries": 3,
  "limit": "4mb",
  "capabilities": {
    "CrossBrowserTesting": {
      "local": true,
      "localIdentifier": "opera-ubuntu-oneiric",
      "screenshots": true,
      "video": true,
      "timeout": 40
    }
  },
  "parallel": {
    "CrossBrowserTesting": 5
  },
  "server": {
    "port": 8000,
    "host": "build.cross-browser-tests-runner.org"
  },
  "browsers": {
    "CrossBrowserTesting": {
      "JS": [
        {
          "os": "Ubuntu",
          "osVersion": "Oneiric",
          "browser": "Opera",
          "browserVersion": "11.52"
        }
      ]
    }
  }
}
