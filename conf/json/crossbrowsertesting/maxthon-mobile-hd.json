{
  "test_file": "tests/html/jasmine/tests.html",
  "framework": "jasmine",
  "retries": 1,
  "limit": "4mb",
  "capabilities": {
    "CrossBrowserTesting": {
      "local": true,
      "screenshots": true,
      "timeout": 60
    }
  },
  "parallel": {
    "CrossBrowserTesting": 5
  },
  "server": {
    "port": 8000,
    "host": "build.cross-browser-tests-runner.org"
  },
  "browsers": {
    "CrossBrowserTesting": {
      "JS": [
        {
          "os": "Android",
          "osVersion": "4.1",
          "browser": "Maxthon Mobile HD",
          "browserVersion": "4.3",
          "device": "Android Galaxy Tab 2"
        }
      ]
    }
  }
}
